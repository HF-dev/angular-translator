{
  "name": "angular-translator",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "true",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n:xlf": "ng xi18n --output-path=src/i18n-xlf --locale en --i18n-format=xlf",
    "extract-i18n:xmb": "ng xi18n --output-path=src/i18n-xmb --locale en --i18n-format=xmb",
    "extract-i18n": "npm run extract-i18n:xlf && npm run extract-i18n:xmb",
    "build:en": "ng build --no-progress --output-path=dist/en -aot -prod --bh /en/ --i18n-file=src/i18n-xlf/messages.en.xlf --i18n-format=xlf --locale=en",
    "build:fr": "ng build --no-progress --output-path=dist/fr -aot -prod --bh /fr/ --i18n-file=src/i18n-xlf/messages.fr.xlf --i18n-format=xlf --locale=fr",
    "build:all": "npm run build:en && npm run build:fr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": ">=4.1.0-beta",
    "@angular/common": ">=4.1.0-beta",
    "@angular/compiler": ">=4.1.0-beta",
    "@angular/core": ">=4.1.0-beta",
    "@angular/forms": ">=4.1.0-beta",
    "@angular/http": ">=4.1.0-beta",
    "@angular/material": "2.0.0-beta.3",
    "@angular/platform-browser": ">=4.1.0-beta",
    "@angular/platform-browser-dynamic": ">=4.1.0-beta",
    "@angular/router": ">=4.1.0-beta",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.3",
    "lodash": "^4.17.4",
    "rxjs": "^5.1.0",
    "xliff": "^1.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0",
    "@angular/compiler-cli": ">=4.1.0-beta",
    "@angular/language-service": ">=4.1.0-beta <5.0.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.0"
  }
}
